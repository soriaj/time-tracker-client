{
   "version": 2,
   "alias": "javiersoria-timetracker-prod",
   "name": "prod-timetracker",
   "routes": [
      {
         "src": "^/static/(.*)",
         "dest": "/static/$1"
      },
      { 
         "src": "/favicon.ico", 
         "dest": "/favicon.ico" 
      },
      {
         "src": ".*",
         "dest": "/index.html"
      }
   ]
}